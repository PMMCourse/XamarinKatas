             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="XamarinFormsKatas.Katas_UI.Kata_e.NuevaPagina">
    <ContentPage.Content>
        <StackLayout>
            <Image x:Name="imagen" WidthRequest="100" HeightRequest="100"></Image>
            <Label x:Name="labelTexto" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>